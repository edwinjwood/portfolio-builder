{
  "env": {
  "node": true,
  "es2021": true,
  "jest": true,
  "commonjs": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
  "no-unused-vars": ["warn", { "argsIgnorePattern": "^_" }],
  "no-console": "off",
  "unused-imports/no-unused-imports": "warn"
  },
  "plugins": ["unused-imports"]
  ,
  "globals": {
    "require": "readonly",
    "module": "readonly",
    "process": "readonly",
    "__dirname": "readonly",
    "global": "readonly",
    "jest": "readonly"
  }
}
